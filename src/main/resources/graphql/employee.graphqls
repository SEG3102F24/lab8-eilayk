type Employee {
    id: ID!
    name: String!
    dateOfBirth: String!
    city: String!
    salary: Float!
    gender: String!
    email: String!
}

type Query {
    employees: [Employee!]!
}

input CreateEmployeeInput {
    name: String!
    dateOfBirth: String!
    city: String!
    salary: Float!
    gender: String!
    email: String!
}

type Mutation {
    newEmployee(createEmployeeInput: CreateEmployeeInput!): Employee!
}

schema {
    query: Query
    mutation: Mutation
}
